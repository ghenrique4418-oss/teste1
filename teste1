    local tween = TweenService:Create(ToggleButton, TweenInfo.new(0.2), {
        BackgroundTransparency = 0
    })
    tween:Play()
end)

ToggleButton.MouseButton1Click:Connect(toggle)

-- ================= SLIDER DE FOV =================

local sliding = false

FovBar.InputBegan:Connect(function(i)
    if i.UserInputType == Enum.UserInputType.MouseButton1
    or i.UserInputType == Enum.UserInputType.Touch then
        sliding = true
        
        -- AnimaÃ§Ã£o do knob
        local tween = TweenService:Create(SliderKnob, TweenInfo.new(0.1), {
            Size = UDim2.new(0, 25, 0, 25),
            BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        })
        tween:Play()
    end
end)

UIS.InputEnded:Connect(function(i)
    if i.UserInputType == Enum.UserInputType.MouseButton1
    or i.UserInputType == Enum.UserInputType.Touch then
        sliding = false
        
        -- Retorna Ã  cor normal
        local tween = TweenService:Create(SliderKnob, TweenInfo.new(0.2), {
            Size = UDim2.new(0, 20, 0, 20),
            BackgroundColor3 = COLORS.Secondary
        })
        tween:Play()
    end
end)

UIS.InputChanged:Connect(function(i)
    if sliding and (
        i.UserInputType == Enum.UserInputType.MouseMovement
        or i.UserInputType == Enum.UserInputType.Touch
    ) then
        local x = math.clamp((i.Position.X - FovBar.AbsolutePosition.X) / FovBar.AbsoluteSize.X, 0, 1)
        FOV = math.floor(x * 100)
        FovFill.Size = UDim2.new(x, 0, 1, 0)
        SliderKnob.Position = UDim2.new(x, -10, 0.5, -10)
        FovValue.Text = tostring(FOV)
        
        -- AnimaÃ§Ã£o suave do valor
        local tween = TweenService:Create(FovValue, TweenInfo.new(0.1), {
            TextColor3 = COLORS.Secondary,
            TextSize = 18
        })
        tween:Play()
        wait(0.1)
        local tween2 = TweenService:Create(FovValue, TweenInfo.new(0.2), {
            TextColor3 = COLORS.Accent,
            TextSize = 16
        })
        
