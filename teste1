    local tween = TweenService:Create(TeamCheckButton, TweenInfo.new(0.2), {
        BackgroundTransparency = 0.1
    })
    tween:Play()
end)

TeamCheckButton.MouseLeave:Connect(function()
    local tween = TweenService:Create(TeamCheckButton, TweenInfo.new(0.2), {
        BackgroundTransparency = 0
    })
    tween:Play()
end)

TeamCheckButton.MouseButton1Click:Connect(toggleTeamCheck)

-- Efeito hover no botÃ£o Aimbot
