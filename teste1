-- Script: Aimbot GUI AvanÃ§ado - Prison Life Edition
-- DescriÃ§Ã£o: Interface grÃ¡fica estilo mobile com tema laranja/branco de Prison Life
-- Recursos: Time Check para evitar amigos, toggle para ativar/desativar

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UIS = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local Camera = workspace.CurrentCamera

local LocalPlayer = Players.LocalPlayer

-- CONFIGURAÃ‡Ã•ES
local AIM_KEY = Enum.KeyCode.V
local SMOOTHNESS = 0.18
local MAX_DISTANCE = 1000

-- VARIÃVEIS
local Enabled = false
local Connection = nil
local FOV = 50
local TeamCheckEnabled = true -- Novo: Time Check ativo por padrÃ£o

-- CORES DO TEMA PRISON LIFE
local COLORS = {
    Background = Color3.fromRGB(240, 240, 240),    -- Branco
    Secondary = Color3.fromRGB(255, 165, 0),       -- Laranja
    Accent = Color3.fromRGB(220, 120, 0),          -- Laranja escuro
    ButtonOn = Color3.fromRGB(46, 204, 113),       -- Verde
    ButtonOff = Color3.fromRGB(231, 76, 60),       -- Vermelho
    Text = Color3.fromRGB(50, 50, 50),             -- Cinza escuro
    TextSecondary = Color3.fromRGB(100, 100, 100), -- Cinza mÃ©dio
    DragArea = Color3.fromRGB(255, 180, 0),        -- Laranja claro
    Border = Color3.fromRGB(200, 200, 200),        -- Cinza claro
    TeamCheckOn = Color3.fromRGB(52, 152, 219),    -- Azul para Team Check ativo
    TeamCheckOff = Color3.fromRGB(149, 165, 166)   -- Cinza para Team Check inativo
}

-- ================= FUNÃ‡ÃƒO TEAM CHECK =================

local function isEnemy(player)
    if not TeamCheckEnabled then
        return true -- Se Team Check estiver desativado, considera todos como inimigos
    end
    
    local localTeam = LocalPlayer.Team
    local targetTeam = player.Team
    
    -- Verifica se ambos tÃªm times
    if localTeam and targetTeam then
        -- Se forem do mesmo time, nÃ£o Ã© inimigo
        if localTeam == targetTeam then
            return false
        end
    end
    
    return true -- Ã‰ inimigo
end

-- ================= FUNÃ‡Ã•ES DE TARGET =================

local function getClosestInFov()
    local closest = nil
    local closestDist = math.huge
    local screenCenter = Vector2.new(Camera.ViewportSize.X/2, Camera.ViewportSize.Y/2)
    local fovRadius = (FOV / 100) * (Camera.ViewportSize.X / 2)

    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then
            -- Verifica se Ã© inimigo usando Team Check
            if isEnemy(plr) then
                local char = plr.Character
                if char then
                    local hum = char:FindFirstChildOfClass("Humanoid")
                    local root = char:FindFirstChild("HumanoidRootPart")

                    if hum and root and hum.Health > 0 then
                        local pos, onScreen = Camera:WorldToViewportPoint(root.Position)
                        if onScreen then
                            local dist = (Vector2.new(pos.X, pos.Y) - screenCenter).Magnitude
                            if dist <= fovRadius then
                                local mag = (Camera.CFrame.Position - root.Position).Magnitude
                                if mag < closestDist and mag < MAX_DISTANCE then
                                    closestDist = mag
                                    closest = root
                                end
                            end
                        end
                    end
                end
            end
        end
    end

    return closest
end

-- ================= FUNÃ‡Ã•ES DO AIMBOT =================

local function start()
    if Connection then Connection:Disconnect() end

    Connection = RunService.RenderStepped:Connect(function()
        if not Enabled then return end

        local target = getClosestInFov()
        if target then
            local cf = CFrame.new(Camera.CFrame.Position, target.Position)
            Camera.CFrame = Camera.CFrame:Lerp(cf, SMOOTHNESS)
        end
    end)
end

local function stop()
    if Connection then
        Connection:Disconnect()
        Connection = nil
    end
end

local function toggle()
    Enabled = not Enabled

    if Enabled then
        start()
        ToggleButton.Text = "ðŸ”« AIMBOT ATIVADO"
        ToggleButton.BackgroundColor3 = COLORS.ButtonOn
        
        -- AnimaÃ§Ã£o do botÃ£o
        local tween = TweenService:Create(ToggleButton, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
            BackgroundColor3 = COLORS.ButtonOn,
            TextColor3 = Color3.fromRGB(255, 255, 255)
        })
        tween:Play()
    else
        stop()
        ToggleButton.Text = "ðŸ”’ AIMBOT DESATIVADO"
        ToggleButton.BackgroundColor3 = COLORS.ButtonOff
        
        -- AnimaÃ§Ã£o do botÃ£o
        local tween = TweenService:Create(ToggleButton, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
            BackgroundColor3 = COLORS.ButtonOff,
            TextColor3 = Color3.fromRGB(255, 255, 255)
        })
        tween:Play()
    end
end

-- ================= FUNÃ‡ÃƒO TEAM CHECK TOGGLE =================

local function toggleTeamCheck()
    TeamCheckEnabled = not TeamCheckEnabled
    
    if TeamCheckEnabled then
        TeamCheckButton.Text = "âœ… IGNORAR AMIGOS"
        TeamCheckButton.BackgroundColor3 = COLORS.TeamCheckOn
        
        -- AnimaÃ§Ã£o do botÃ£o
        local tween = TweenService:Create(TeamCheckButton, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
            BackgroundColor3 = COLORS.TeamCheckOn,
            TextColor3 = Color3.fromRGB(255, 255, 255)
        })
        tween:Play()
        
        print("ðŸŽ¯ Team Check: ATIVADO - Aimbot ignorarÃ¡ players do seu time")
    else
        TeamCheckButton.Text = "âŒ ATIRAR EM TODOS"
        TeamCheckButton.BackgroundColor3 = COLORS.TeamCheckOff
        
        -- AnimaÃ§Ã£o do botÃ£o
        local tween = TweenService:Create(TeamCheckButton, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
            BackgroundColor3 = COLORS.TeamCheckOff,
            TextColor3 = Color3.fromRGB(255, 255, 255)
        })
        tween:Play()
        
        print("ðŸŽ¯ Team Check: DESATIVADO - Aimbot mirarÃ¡ em todos os players")
    end
end

